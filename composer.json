{
    "name": "mibo/properties",
    "type": "library",
    "description": "PHP Library for generic and/or custom properties",
    "keywords": [
        "PHP",
        "Properties",
        "Property"
    ],
    "authors": [
        {
            "name": "Michal Boris"
        }
    ],
    "autoload": {
        "psr-4": {
            "\\MiBo\\Properties\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "\\MiBo\\Properties\\Tests\\": [
                "tests/Core/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "psr/log": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-invoker": "^3.1",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "platform-check": true,
        "lock": true
    }
}
